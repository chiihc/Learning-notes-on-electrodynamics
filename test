\begin{align}
\boldsymbol{\nabla}^2\boldsymbol{A}
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_i\boldsymbol{e}_i}}{\partial s}\right)
+\frac{1}{s^2}\dfrac{{\partial^2 A_i\boldsymbol{e}_i}}{\partial \phi^2}
+\dfrac{{\partial^2 A_i\boldsymbol{e}_i}}{\partial z^2}\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s\boldsymbol{e}_s}}{\partial s}\right)
+\frac{1}{s^2}\dfrac{{\partial^2 A_s\boldsymbol{e}_s}}{\partial \phi^2}
+\dfrac{{\partial^2 A_s\boldsymbol{e}_s}}{\partial z^2}\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s}}{\partial s}\right)\boldsymbol{e}_s
+\frac{1}{s^2}\dfrac{\partial}{\partial \phi}\left(A_s\dfrac{{\partial \boldsymbol{e}_s}}{\partial \phi}
+\boldsymbol{e}_s\dfrac{{\partial A_s}}{\partial \phi}\right)
+\dfrac{{\partial^2 A_s}}{\partial z^2}\boldsymbol{e}_s\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s}}{\partial s}\right)\boldsymbol{e}_s
+\frac{1}{s^2}\dfrac{\partial}{\partial \phi}\left(A_s\boldsymbol{e}_\phi
+\boldsymbol{e}_s\dfrac{{\partial A_s}}{\partial \phi}\right)
+\dfrac{{\partial^2 A_s}}{\partial z^2}\boldsymbol{e}_s\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s}}{\partial s}\right)\boldsymbol{e}_s
+\frac{1}{s^2}\left(A_s\dfrac{\partial \boldsymbol{e}_\phi}{\partial \phi}
+\boldsymbol{e}_\phi\dfrac{\partial A_s}{\partial \phi}
+\dfrac{\partial\boldsymbol{e}_s}{\partial \phi}\dfrac{{\partial A_s}}{\partial \phi}
+\boldsymbol{e}_s\dfrac{{\partial^2 A_s}}{\partial \phi^2}\right)
+\dfrac{{\partial^2 A_s}}{\partial z^2}\boldsymbol{e}_s\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s}}{\partial s}\right)\boldsymbol{e}_s
+\frac{1}{s^2}\left(-A_s\boldsymbol{e}_s
+\boldsymbol{e}_\phi\dfrac{\partial A_s}{\partial \phi}
+\boldsymbol{e}_\phi\dfrac{{\partial A_s}}{\partial \phi}
+\boldsymbol{e}_s\dfrac{{\partial^2 A_s}}{\partial \phi^2}\right)
+\dfrac{{\partial^2 A_s}}{\partial z^2}\boldsymbol{e}_s\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_s
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_s}}{\partial s}\right)\boldsymbol{e}_s
+\frac{1}{s^2}\left(-A_s\boldsymbol{e}_s
+2\boldsymbol{e}_\phi\dfrac{\partial A_s}{\partial \phi}
+\boldsymbol{e}_s\dfrac{{\partial^2 A_s}}{\partial \phi^2}\right)
+\dfrac{{\partial^2 A_s}}{\partial z^2}\boldsymbol{e}_s\\
\boldsymbol{\nabla}^2\boldsymbol{A}_\phi
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_\phi}}{\partial s}\right)\boldsymbol{e}_\phi
+\frac{1}{s^2}\dfrac{{\partial}}{\partial \phi}\left(A_\phi\dfrac{{\partial \boldsymbol{e}_\phi }}{\partial \phi}
+\boldsymbol{e}_\phi\dfrac{{\partial A_\phi }}{\partial \phi}\right)
+\dfrac{{\partial^2 A_\phi}}{\partial z^2}\boldsymbol{e}_\phi\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_\phi
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_\phi}}{\partial s}\right)\boldsymbol{e}_\phi
+\frac{1}{s^2}\dfrac{{\partial}}{\partial \phi}\left(-A_\phi\boldsymbol{e}_s
+\boldsymbol{e}_\phi\dfrac{{\partial A_\phi }}{\partial \phi}\right)
+\dfrac{{\partial^2 A_\phi}}{\partial z^2}\boldsymbol{e}_\phi\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_\phi
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_\phi}}{\partial s}\right)\boldsymbol{e}_\phi
+\frac{1}{s^2}\left(-\boldsymbol{e}_s\dfrac{{\partial A_\phi}}{\partial \phi}
-A_\phi\dfrac{{\partial\boldsymbol{e}_s}}{\partial \phi}
+\boldsymbol{e}_\phi\dfrac{{\partial^2 A_\phi }}{\partial \phi^2}
+\dfrac{{\partial\boldsymbol{e}_\phi}}{\partial \phi}\dfrac{{\partial A_\phi }}{\partial \phi}\right)
+\dfrac{{\partial^2 A_\phi}}{\partial z^2}\boldsymbol{e}_\phi\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_\phi
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_\phi}}{\partial s}\right)\boldsymbol{e}_\phi
+\frac{1}{s^2}\left(-\boldsymbol{e}_s\dfrac{{\partial A_\phi}}{\partial \phi}
-A_\phi\boldsymbol{e}_\phi
+\boldsymbol{e}_\phi\dfrac{{\partial^2 A_\phi }}{\partial \phi^2}
-\boldsymbol{e}_s\dfrac{{\partial A_\phi }}{\partial \phi}\right)
+\dfrac{{\partial^2 A_\phi}}{\partial z^2}\boldsymbol{e}_\phi\nonumber\\
\boldsymbol{\nabla}^2\boldsymbol{A}_\phi
&=\frac{1}{s}\frac{\partial}{\partial s}\left(s\dfrac{{\partial A_\phi}}{\partial s}\right)\boldsymbol{e}_\phi
+\frac{1}{s^2}\left(-2\boldsymbol{e}_s\dfrac{{\partial A_\phi}}{\partial \phi}
-A_\phi\boldsymbol{e}_\phi
+\boldsymbol{e}_\phi\dfrac{{\partial^2 A_\phi }}{\partial \phi^2}\right)
+\dfrac{{\partial^2 A_\phi}}{\partial z^2}\boldsymbol{e}_\phi
\end{align}